{

    "manifest_version": 2,
    "name": "Sleepyheads",
    "version": "1.0",

    "description": "The app, Sleepyheads, in your browser. Perfect for a good nights sleep.",
    "homepage_url": "https://busaruba2011.github.io/Sleepyheads",
    "icons": {
        "48": "icons/48.png"
    },

    "permissions": [
        "activeTab"
    ],

    "browser_action": {
        "default_icon": "icons/48.png",
        "default_title": "Sleepyheads",
        "default_popup": "popup/index.html"
    }




}